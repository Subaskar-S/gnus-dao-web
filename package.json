{
  "name": "gnus-dao-governance",
  "version": "1.0.0",
  "description": "GNUS DAO governance platform with quadratic voting, multi-chain support, and Diamond pattern integration",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:turbo": "next dev --port 3000 --turbo",
    "build": "next build",
    "build:production": "cross-env NODE_ENV=production STATIC_EXPORT=true CLOUDFLARE_PAGES=true next build",
    "start": "next start --port 3000",
    "preview": "yarn build && yarn start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "validate": "yarn type-check && yarn lint && yarn test:ci",
    "deploy:cloudflare": "wrangler pages deploy out --project-name=gnus-dao-web",
    "deploy:prepare": "yarn build:production",
    "clean": "cross-env rimraf .next out dist node_modules/.cache",
    "clean:all": "yarn clean && cross-env rimraf node_modules yarn.lock",
    "prepare": "husky install || true",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "postinstall": "patch-package || true"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@pinata/sdk": "^2.1.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@reduxjs/toolkit": "^2.9.0",
    "@reown/appkit": "^1.8.4",
    "@reown/appkit-adapter-ethers": "^1.8.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@tanstack/react-query": "^5.59.0",
    "@walletconnect/ethereum-provider": "^2.21.8",
    "@walletconnect/keyvaluestorage": "^1.1.1",
    "@walletconnect/modal": "^2.7.0",
    "@walletconnect/sign-client": "^2.17.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "ethers": "^6.13.2",
    "framer-motion": "^11.11.7",
    "ipfs-http-client": "^60.0.1",
    "lucide-react": "^0.451.0",
    "multiformats": "^13.4.1",
    "next": "^14.2.5",
    "next-themes": "^0.4.6",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.6.0",
    "react-is": "^19.2.0",
    "react-redux": "^9.2.0",
    "recharts": "^3.3.0",
    "siwe": "^2.3.2",
    "tailwind-merge": "^2.5.3",
    "tailwindcss-animate": "^1.0.7",
    "viem": "^2.37.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@ethersproject/sha2": "^5.8.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.5.2",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-is": "^19",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "patch-package": "^8.0.0",
    "playwright": "^1.56.1",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.4",
    "vercel": "^34.4.0",
    "wrangler": "^4.40.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "dao",
    "governance",
    "quadratic-voting",
    "walletconnect",
    "ethereum",
    "web3",
    "diamond-pattern",
    "gnus",
    "base-network",
    "skale",
    "polygon",
    "multi-chain",
    "enterprise"
  ],
  "author": "Subaskar_S",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GeniusVentures/gnus-dao-governance.git"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn validate"
    }
  },
  "packageManager": "yarn@4.9.1"
}
