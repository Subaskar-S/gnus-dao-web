{
  "name": "gnus-dao-governance",
  "version": "2.0.0",
  "description": "Enterprise-grade GNUS DAO governance platform with quadratic voting, multi-chain support, and Diamond pattern integration",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "build:static": "next build && next export",
    "build:cloudflare": "node scripts/build-cloudflare.js",
    "start": "next start --port 3000",
    "preview": "yarn build && yarn start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "validate": "yarn type-check && yarn lint && yarn test:ci",
    "docker:build": "docker build -t gnus-dao-governance .",
    "docker:run": "docker run -p 3000:3000 gnus-dao-governance",
    "docker:compose": "docker-compose up -d",
    "docker:compose:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rm -rf .next out dist node_modules/.cache",
    "health-check": "node scripts/health-check.js",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@phosphor-icons/webcomponents": "^2.1.5",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@reduxjs/toolkit": "^2.9.0",
    "@reown/appkit": "^1.8.4",
    "@reown/appkit-adapter-ethers": "^1.8.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@tanstack/react-query": "^5.59.0",
    "@types/qrcode": "^1.5.5",
    "@walletconnect/ethereum-provider": "^2.21.8",
    "@walletconnect/keyvaluestorage": "^1.1.1",
    "@walletconnect/modal": "^2.7.0",
    "@walletconnect/sign-client": "^2.17.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "ethers": "^6.13.2",
    "framer-motion": "^11.11.7",
    "lucide-react": "^0.451.0",
    "next": "^14.2.5",
    "next-themes": "^0.4.6",
    "preact": "^10.27.1",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.6.0",
    "react-redux": "^9.2.0",
    "siwe": "^2.3.2",
    "tailwind-merge": "^2.5.3",
    "tailwindcss-animate": "^1.0.7",
    "viem": "^2.37.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ethersproject/sha2": "^5.8.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "pino-pretty": "^13.1.1",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.4",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "dao",
    "governance",
    "quadratic-voting",
    "walletconnect",
    "ethereum",
    "web3",
    "diamond-pattern",
    "gnus",
    "base-network",
    "skale",
    "polygon",
    "multi-chain",
    "enterprise"
  ],
  "author": "Genius Ventures",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GeniusVentures/gnus-dao-governance.git"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn validate"
    }
  }
}
